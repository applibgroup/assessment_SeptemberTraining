<?xml version="1.0" encoding="utf-8"?>
<DirectionalLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_parent"
    ohos:width="match_parent"
    ohos:background_element="$media:regis_back"
    ohos:orientation="vertical">


    <Text
        ohos:id="$+id:student_idcard"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:text="Student ID Card"
        ohos:text_size="35fp"
        ohos:text_font="sans-serif-medium"
        ohos:layout_alignment="center"
        ohos:top_margin="60fp"


        />

    <DependentLayout

        xmlns:ohos="http://schemas.huawei.com/res/ohos"
        ohos:height="250fp"
        ohos:width="280fp"
        ohos:left_margin="40fp"
        ohos:top_margin="100fp"
        ohos:background_element="$graphic:directinal_layout_card"
        >



    </DependentLayout>

</DirectionalLayout>